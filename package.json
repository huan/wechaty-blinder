  },
  "homepage": "https://github.com/zixia/wechaty-blinder#readme",
  "devDependencies": {
    "@types/app-root-path": "^1.0.0",
    "@types/blue-tape": "^0.1.0",
    "@types/node": "^8.0.0",
    "@types/read-pkg-up": "^2.0.0",
    "blue-tape": "^1.0.0",
    "shx": "^0.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
